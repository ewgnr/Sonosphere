import ipywidgets as widgets
from IPython.display import display

uploader = widgets.FileUpload(
    accept='.txt',
    multiple=True,
    description='Upload Text Files'
)
display(uploader)


for uploaded_file in uploader.value:
    name = uploaded_file['name']
    content = uploaded_file['content']      # This is a memoryview object
    size = uploaded_file['size']
    print(f"File: {name}, Size: {size} bytes")
    # To get content as bytes:
    data_bytes = content.tobytes()



